<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <title>ccPhp Micro-framework</title>
  <parser>
    <default-package-name>ccPhp</default-package-name>
    <target>docs</target>
  </parser>
  <transformer>
    <target>docs</target>
  </transformer>
  <files>
    <directory>.</directory>
    <file>vendor/psr/log/Psr/Log/*</file>
    <file>vendor/composer/ClassLoader.php</file>
	<ignore>composer.json</ignore>
	<ignore>vendor/*</ignore>
  </files>
</phpdoc>
